{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start-dev": "cross-env ENV=dev nodemon src/server.ts",
    "start-test": "cross-env ENV=test nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
    "test": "db-migrate --env test up && cross-env ENV=test jasmine-ts --config jasmine.json || db-migrate --env test down",
    "db-up": "db-migrate up -m ./migrations",
    "db-down": "db-migrate reset -m ./migrations",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "pg": "^8.5.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^18.7.6",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^6.1.4",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.2.9"
  }
}
